{
  "name": "energy-data-events-ingest-connector",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSourceConnector",
    "tasks.max": "1",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "errors.retry.timeout": "60000",
    "errors.retry.delay.max.ms": "300000",
    "errors.tolerance": "none",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "connection.url": "jdbc:postgresql://postgres-service.green-energy-tracker:5432/green-energy-tracker?currentSchema=microservices",
    "connection.user": "spring-training_user",
    "connection.password": "spring-training",
    "connection.attempts": "3",
    "connection.backoff.ms": "1000",
    "table.whitelist": "microservices.energy_data",
    "schema.pattern": "microservices",
    "mode": "incrementing",
    "incrementing.column.name": "id",
    "poll.interval.ms": "5000",
    "topic.creation.default.partitions": "1",
    "topic.creation.default.replication.factor": "1",
    "topic.creation.enable": "true",
    "transforms": "MakeKey,ExtractKeyField",
    "transforms.MakeKey.type": "org.apache.kafka.connect.transforms.ValueToKey",
    "transforms.MakeKey.fields": "id",
    "transforms.ExtractKeyField.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
    "transforms.ExtractKeyField.field": "id"
  }
}